#include<stdio.h>
int main()
{
	int a[50],i,n,pos,flag=0,key;
	printf("enter n");
	scanf("%d",&n);
	printf("enter the elements of an array");
	for(i=0;i<n;i++)
	scanf("%d",&a[i]);
	printf("enter the key value");
	scanf("%d",&key);
	for(i=0;i<n;i++)
	{
	if(a[i]==key)
	{
		flag=1;
		pos=i;
		break;
	}
}
	if(flag==1)
	printf("key element is found at position is %d",pos);
	printf("key element is not found");
	return 0;
}
